page {
  height: 100vh;
}
.tubiao .top {
  width: 750rpx;
  height: 300rpx;
  background-color: var(--jizhangColor);
  padding: 0 20rpx;
}
.tubiao .top .filter {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 100rpx;
}
.tubiao .top .filter .filter_time {
  font-size: 30rpx;
}
.tubiao .top .filter .filter_isactive .van-radio-group {
  display: flex;
}
.tubiao .top .filter .filter_isactive .van-radio-group .van-button {
  width: 120rpx;
  height: 60rpx;
  border-radius: 20rpx;
}
.tubiao .top .total {
  height: 200rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.tubiao .top .total .text {
  padding-left: 15rpx;
  color: #302f2f;
  margin-bottom: 15rpx;
}
.tubiao .top .total .money {
  font-size: 40rpx;
}
.tubiao .container {
  width: 750rpx;
  height: 700rpx;
}
.tubiao ec-canvas {
  width: 100%;
  height: 700px;
}
.tubiao .container1 {
  width: 750rpx;
  height: 600rpx;
}
